<%= form_with model: @kitten do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :age %>
  <%= f.number_field :age %>
  <%= f.label :cuteness %>
  <%= f.range_field :cuteness, in: 1..10 %>
  <%= f.label :softness %>
  <%= f.range_field :softness, in: 1..10 %>
  <%= f.submit %>
<% end %>